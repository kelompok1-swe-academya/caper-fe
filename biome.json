{
	"files": {
		"ignore": [],
		"ignoreUnknown": false
	},
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"formatter": {
		"enabled": true,
		"indentStyle": "tab",
		"indentWidth": 2,
		"lineEnding": "lf",
		"lineWidth": 80
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "double"
		}
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"organizeImports": {
		"enabled": true
	},
	"vcs": {
		"clientKind": "git",
		"defaultBranch": "main",
		"enabled": true,
		"useIgnoreFile": true
	},
	"json": {
		"linter": {
			"enabled": true
		},
		"formatter": {
			"enabled": true
		},
		"parser": {
			"allowComments": true,
			"allowTrailingCommas": true
		}
	}
}
